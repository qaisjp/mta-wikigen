{
    "name": "Multi Theft Auto wikigen",
    "version": "1.0",
    "description": "Quickly create Lua pages on the MTA wiki",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "hot-reload.js"
        ]
    },
    "content_scripts": [
        {
            "matches": ["https://wiki.mtasa.com/*", "https://wiki.multitheftauto.com/*"],
            "js": ["contentScript.js"]
            // "css": ["styles.css"]
        }
    ]
}